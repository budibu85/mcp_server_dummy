{
  "servers": {
    "MCP-Server-Dummy": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "./MCP.Server.Dummy/MCP.Server.Dummy.csproj"
      ]
    },
    "MCP-Server-Dummy-Docker": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcpserverdummy:latest"
      ]
    }
  }
}
